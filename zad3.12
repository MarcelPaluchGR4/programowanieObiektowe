using System;

namespace ConsoleApplication1
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            int i, wynik;
            wynik = 0;
            i = int.Parse(Console.ReadLine());
            while (i != 0)
            {
                wynik += i;
                i = int.Parse(Console.ReadLine());
            }
            Console.WriteLine(wynik);
        }
    }
}
